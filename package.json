{
  "name": "side_client_app",
  "version": "0.0.0",
  "main": "main.ts",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "electron:dev": "vite build && electron .",
    "electron:build": "vite build --mode production && electron-builder build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false"
  },
  "build": {
    "appId": "ServiceApp",
    "win": {
      "target": "nsis"
    },
   
    "files": ["**/*", "dist/**/*"],
    "directories": {
      "buildResources": "dist",
      "output": "release"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
