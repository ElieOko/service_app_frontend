    "electron": "^28.1.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
